{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "components/*"
      ],
      "$/*": [
        "utils/*"
      ],
      "db": [
        "lib/prisma"
      ]
    },
    "plugins": [
      {
        "name": "typescript-plugin-css-modules"
      }
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "tsBuildInfoFile": ".next/cache/.tsbuildinfo"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}